          </image>
          <image>
            <type>screenshot</type>
            <image-file>galaxian/0000.png</image-file>
          </image>
        </images>
        <video>galaxian/0000.mpg</video>
        <categories>
          <category>
            <name>Genre</name>
            <value>Shoot 'em Up</value>
          </category>
        </categories>
      </game>
      <game>
        <name>1942</name>
        <emulator>mame</emulator>
        <rom-image>1942a</rom-image>
        <images>
          <image>
            <type>logo</type>
            <image-file>1942a/1942logo.png</image-file>
          </image>
          <image>
            <type>screenshot</type>
