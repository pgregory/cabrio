          </image>
        </images>
        <categories>
          <category>
            <name>Genre</name>
            <value>Shoot 'em Up</value>
          </category>
        </categories>
      </game>
      <game>
        <name>19XX</name>
        <emulator>mame</emulator>
        <rom-image>19xx</rom-image>
        <images>
          <image>
            <type>logo</type>
            <image-file>19xx/19xx.png</image-file>
          </image>
          <image>
            <type>screenshot</type>
            <image-file>19xx/0002.png</image-file>
          </image>
        </images>
        <categories>
          <category>
            <name>Genre</name>
            <value>Shoot 'em Up</value>
          </category>
        </categories>
      </game>
      <game>
        <name>Afterburner II</name>
        <emulator>mame</emulator>
        <rom-image>aburner2</rom-image>
        <images>
          <image>
            <type>logo</type>
            <image-file>aburner2/after2.png</image-file>
          </image>
          <image>
            <type>screenshot</type>
            <image-file>aburner2/0000.png</image-file>
