<?xml version="1.0" encoding="UTF-8"?>
<cabrio-config>
  <interface>
    <full-screen>false</full-screen>
    <theme>carousel</theme>
    <frame-rate>60</frame-rate>
    <screen>
      <width>640</width>
      <height>480</height>
      <rotation>0</rotation>
      <flip-horizontal>0</flip-horizontal>
      <flip-vertical>0</flip-vertical>
    </screen>
    <graphics>
      <quality>high</quality>
      <max-image-width>512</max-image-width>
      <max-image-height>512</max-image-height>
    </graphics>
    <controls>
      <event>
        <name>up</name>
        <value>up</value>
        <device>
          <type>keyboard</type>
          <id>1</id>
        </device>
      </event>
      <event>
        <name>down</name>
        <value>down</value>
        <device>
          <type>keyboard</type>
          <id>1</id>
        </device>
      </event>
      <event>
        <name>left</name>
        <value>left</value>
        <device>
          <type>keyboard</type>
          <id>1</id>
        </device>
      </event>
      <event>
        <name>right</name>
        <value>right</value>
        <device>
          <type>keyboard</type>
          <id>1</id>
        </device>
      </event>
      <event>
        <name>select</name>
        <value>space</value>
        <device>
          <type>keyboard</type>
          <id>1</id>
        </device>
      </event>
      <event>
        <name>back</name>
        <value>backspace</value>
        <device>
          <type>keyboard</type>
          <id>1</id>
        </device>
      </event>
      <event>
        <name>quit</name>
        <value>q</value>
        <device>
          <type>keyboard</type>
          <id>1</id>
        </device>
      </event>
    </controls>
  </interface>
  <locations>
    <location>
      <type>screenshot</type>
      <directory>/Users/pgregory/Library/Application Support/MAME OS X/Screenshots/</directory>
    </location>
    <location>
      <type>video</type>
      <directory>/Users/pgregory/Library/Application Support/MAME OS X/Screenshots/</directory>
    </location>
    <location>
      <type>logo</type>
      <directory>/Users/pgregory/Library/Application Support/MAME OS X/Cabinet Art/</directory>
    </location>
  </locations>
  <emulators>
    <emulator>
      <name>mame</name>
      <display-name>MAME</display-name>
      <executable>/Applications/MAME OS X.app/Contents/MacOS/MAME OS X</executable>
      <params>
        <param>
          <name>-Game</name>
        </param>
      </params>
    </emulator>
  </emulators>
  <game-list>
    <name>My List</name>
    <games>
      <game>
        <name>'88 Games</name>
        <emulator>mame</emulator>
        <rom-image>88games</rom-image>
        <images>
          <image>
            <type>logo</type>
            <image-file>88games/title.png</image-file>
          </image>
          <image>
            <type>screenshot</type>
            <image-file>88games/0003.png</image-file>
          </image>
        </images>
        <categories>
          <category>
            <name>Genre</name>
            <value>Sports</value>
          </category>
        </categories>
      </game>
      <game>
        <name>Pac Man</name>
        <emulator>mame</emulator>
        <rom-image>pacmanbl</rom-image>
        <images>
          <image>
            <type>logo</type>
            <image-file>/Users/pgregory/Downloads/pacman_logo.gif</image-file>
          </image>
          <image>
            <type>screenshot</type>
            <image-file>pacmanbl/0003.png</image-file>
          </image>
        </images>
        <categories>
